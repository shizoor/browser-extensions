{
  "manifest_version": 3,
  "name": "dark-x",
  "version": "1.0",
  "description": "Makes twitter-x dark",
  "web_accessible_resources": [
    {
    "resources": ["images/*.png"],
    "matches": ["<all_urls>"]
    }
],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
